                                    <TableColumn fx:id="ratingID" prefWidth="75.0" text="ID" />
                                    <TableColumn fx:id="ratingResID" prefWidth="75.0" text="resID" />
                                    <TableColumn fx:id="ratingCusID" prefWidth="75.0" text="cusID" />
                                    <TableColumn fx:id="ratingValue" prefWidth="75.0" text="Value" />
                                 </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
               </tabs>
            </TabPane>
         </children>
      </GridPane>
   </center>
