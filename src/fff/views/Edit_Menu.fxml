<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fff.views.Edit_Menu">
   <children>
      <TableView fx:id="foodTableView" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0">
        <columns>
          <TableColumn fx:id="foodName" prefWidth="75.0" text="Food" />
          <TableColumn fx:id="foodPrice" prefWidth="75.0" text="Price" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ButtonBar layoutX="227.0" layoutY="261.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="25.0" prefWidth="209.0">
         <buttons>
            <Button fx:id="editFood" mnemonicParsing="false" text="Edit Food" />
            <Button fx:id="removeFood" mnemonicParsing="false" prefHeight="25.0" prefWidth="97.0" text="Remove Food" />
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
